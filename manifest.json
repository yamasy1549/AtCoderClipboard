{
    "manifest_version": 2,
    "name": "AtCoderClipboard",
    "description": "AtCoderの入力例をワンクリックでコピーする",
    "version": "1.1.0",
    "icons": {
        "16": "images/icon/icon_16.png",
        "48": "images/icon/icon_48.png",
        "128": "images/icon/icon_128.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*.contest.atcoder.jp/tasks/*"],
            "js": ["js/jquery.js", "js/clipboard.js", "js/main.js"],
            "run_at": "document_idle"
        }
    ]
}
